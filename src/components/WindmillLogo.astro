<style>
  .openagi-logo {
    width: var(--size);
    height: var(--size);
    background: transparent;
    display: flex;
    justify-content: center;
    align-items: center;
    /* Floating animation */
    animation: float 4s ease-in-out infinite;
    /* Blue drop shadow */
    filter: drop-shadow(0 0 15px rgba(0, 212, 255, 0.4));
    transition: all 0.3s ease;
  }

  .logo-group {
    transform-origin: center;
    /* Gentle rotation */
    animation: gentle-rotate var(--speed) linear infinite;
  }

  .main-shape {
    /* Breathing opacity effect */
    animation: shape-breath 4s ease-in-out infinite alternate;
  }
  
  .center-accent {
    /* Blinking effect like lobster eyes */
    animation: glow-pulse 3s ease-in-out infinite;
  }

  /* Floating animation - vertical movement */
  @keyframes float {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-8px); }
  }

  /* Gentle rotation animation */
  @keyframes gentle-rotate {
    from { transform: rotate(0deg); }
    to { transform: rotate(360deg); }
  }

  /* Shape breathing */
  @keyframes shape-breath {
    from { opacity: 0.8; }
    to { opacity: 1; }
  }
  
  /* Eye-like blinking pulse */
  @keyframes glow-pulse {
    0%, 90% { opacity: 1; }
    95%, 100% { opacity: 0.3; }
  }
  
  /* Hover effects */
  .openagi-logo:hover {
    transform: scale(1.1) translateY(0);
    /* Shift to cyan glow on hover */
    filter: drop-shadow(0 0 20px rgba(0, 229, 204, 0.6));
  }
  
  .openagi-logo:hover .logo-group {
    /* Accelerate rotation on hover */
    animation-duration: 2s;
  }
</style>
